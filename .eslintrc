{
  "plugins": [
    "react"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true,
    "jest": true,
    "es6": true
  },
  "rules": {
    "react/forbid-prop-types": 1,
    "react/self-closing-comp": "warn",
    "react/prefer-stateless-function": 0,
    "jsx-a11y/href-no-hash": 0,
    "react/jsx-filename-extension": 0,
    "arrow-body-style": 0,
    "padded-blocks": "warn",
    "comma-dangle": 0,
    "no-unused-vars": 1
  },
  "extends": ["airbnb", "plugin:react/recommended"]
}

